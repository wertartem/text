simbol =asddd
#simbol = ('oe', 'uo', 'fi', 'oa', "ru","riikka.mehi")
with open("Auton_vuokraus.txt", "r") as fin: 
    www = fin.read()
for string in www.split('\n'):
    for i in simbol:
        string = string.replace(i, '')
    if string:
        with open("Auton_vuokraus_save.txt",'a') as file_handler23:
            file_handler23.write(string+"\n")
