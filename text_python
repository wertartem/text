in_file_name = 'mail1.txt'
out_file_name = 'Auton_vuokraus.txt'
with open(in_file_name) as fin:
    er = fin.read()
    for lines in er.split('\n'):
        for linezz in lines.split(','):
            with open(out_file_name, 'a') as finsave:
                finsave.write(linezz +"#\n")
